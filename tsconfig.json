{
	"compilerOptions": {
		"target": "ES6",
		"baseUrl": ".",
		"paths": {
			"@/*": ["./app/*"],
			"@/assets/*": ["./app/assets/*"],
			"@/components/*": ["./app/components/*"],
			"@/api/*": ["./app/api/*"],
			"@/navigators/*": ["./app/navigators/*"]
		}
	},
	"extends": "expo/tsconfig.base",
	"include": [
		"**/*.ts",
		"**/*.tsx",
		".expo/types/**/*.ts",
		"expo-env.d.ts"
	]
}
